{
  "name": "lastsummer",
  "version": "1.0.0",
  "description": "Makes it easy to maintain a changelog for your project with interactivity & bulk editing",
  "main": "src/index.js",
  "bin": {
    "lastsummer": "./bin/executable.js"
  },
  "preferGlobal": true,
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha './test/**/*.test.js'",
    "changelog": "node ./bin/executable.js",
    "releaselog": "node ./bin/executable.js --release"
  },
  "keywords": [
    "changelog",
    "interactive",
    "CLI",
    "markdown",
    "hooks",
    "release"
  ],
  "author": "Pushkar DK <pushkar.dk@hotstar.com>",
  "license": "MIT",
  "dependencies": {
    "inquirer": "^6.3.1",
    "node-watch": "^0.6.2",
    "open-in-editor": "^2.2.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "sinon": "^7.3.2"
  }
}
